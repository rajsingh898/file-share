{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
        
            "src": "/(.*)",
            "dest": "/"
          },
      {

        "src": "/upload",
        "dest": "/api/upload.js"
      },
      {
        "src": "/download/(.*)",
        "dest": "/api/download.js"
      }
    ]
  }
  