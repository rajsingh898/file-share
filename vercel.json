{
    "version": 2,
    "builds": [
      {
        "src": "api/upload.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/download.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/upload",
        "dest": "/api/upload.js"
      },
      {
        "src": "/download",
        "dest": "/api/download.js"
      }
    ]
  }
  